# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

# Disable use of input/output files here so that when we build it includes any changes to the development pod
# rather than having to manually clean and rebuuild each time
install! 'cocoapods', :disable_input_output_paths => true

target 'ETDASSI' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  pod 'SwiftyJSON'
  pod 'Alamofire'
  pod 'SVPinView'
  pod 'KeychainSwift'
  pod "KRProgressHUD"
  pod 'Firebase/Analytics'
  pod 'Firebase/Messaging'
  pod 'Firebase/RemoteConfig'
  pod 'Presentr'
  pod 'RealmSwift', '~> 10.0'
  pod 'Sentry', :git => 'https://github.com/getsentry/sentry-cocoa.git', :tag => '7.4.6'
  
end
